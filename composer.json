{
    "name": "madbob/scambisolidali",
    "description": "CeloCelo",
    "keywords": ["economia solidale", "donazioni"],
    "license": "AGPL-3.0-or-later",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "laravel/framework": "^7.0",
        "watson/bootstrap-form": "^1.4",
        "aws/aws-sdk-php": "^3.20",
        "diversen/image-auto-rotate": "^1.0",
        "barryvdh/laravel-debugbar": "^3.1",
        "madbob/laravel-notification-mobyt": "^0.1",
        "nunomaduro/collision": "^4.1",
        "laravel/tinker": "^2.0",
        "facade/ignition": "^2.0",
        "laravel/ui": "^2.0",
        "league/flysystem-aws-s3-v3": "^1.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "^8.5",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*",
        "filp/whoops": "~2.0",
        "nunomaduro/larastan": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Helpers/dates.php",
            "app/Helpers/formats.php",
            "app/Helpers/custom.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
